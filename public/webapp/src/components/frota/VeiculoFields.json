[
    {
        "label": "Código",
        "name": "id",
        "desc": "Código do Cliente",
        "bclass": "col-md-1",
        "type": "input-text",
        "attribs": {
            "readonly": "readonly"
        }
    },
    {
        "label": "Nº Frota",
        "name": "numero_frota",
        "desc": "Identificação do veiculo na frota ",
        "bclass": "col-md-2",
        "type": "input-text"
    },
    {
        "label": "Placa/Série",
        "name": "placa",
        "desc": "Placa do Veículo ou Número de série",
        "bclass": "col-md-2",
        "type": "input-text"
    },
    {
        "label": "Tipo",
        "name": "tipo_id",
        "desc": "Tipo de veículo",
        "bclass": "col-md-2",
