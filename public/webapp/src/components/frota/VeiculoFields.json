        "bclass": "col-md-2",
        "type": "input-text"
    },
    {
        "label": "Cor",
        "name": "cor",
        "desc": "Cor",
        "bclass": "col-md-2",
        "type": "input-text"
    },
    {
        "label": "Rastreador",
        "name": "rastreador_esn",
        "desc": "Rastreador",
        "bclass": "col-md-4",
        "type": "input-rastreador"
    },
    {
