        "filtros": "digitos"
    },
    {
        "label": "ICCID",
        "name": "iccid",
        "desc": "ICCID",
        "bclass": "col-md-3",
        "type": "InputMask",
        "attribs": {
            "mask": "#####################"
        }
    },
    {
        "label": "Operadora Gsm",
        "name": "operadora_gsm",
