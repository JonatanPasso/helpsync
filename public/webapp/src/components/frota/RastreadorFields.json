        "bclass": "col-md-2",
        "type": "InputSelect",
        "options": [
            "CLARO",
            "VIVO",
            "OI",
            "TIM",
            "NEXTEL",
            "ALGAR"
        ]
    },
    {
        "label": "Estoque",
        "name": "estoque",
        "desc": "Estoque",
        "bclass": "col-md-2",
        "type": "InputMask",
        "attribs": {
            "mask": "#########"
        }
    },
    {
        "label": "Status",
        "name": "status",
        "desc": "Status do Rastreador",
        "bclass": "col-md-3",
        "type": "InputSelect",
        "options": [
            "ATIVO",
